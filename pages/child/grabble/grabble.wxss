/* pages/child/grabble/grabble.wxss */
/*头部样式*/

page{
  width: 100%;
  height: 100%;
}

.pageAll{
   width: 100%;
  height: 100%;
  background: #F4F4F4;
}
.psection1{
  background: #ffffff;
  padding: 12rpx 30rpx;
  font-size: 28rpx;
  color: #000000;
  display: flex;
  justify-content:space-between;
}
.tcount1{
  position: relative;
  top:-10rpx;
}
.Pinput{
  position: relative;
}
.Pinput image{
  position: absolute;
  left: 15rpx;
  top: 5rpx;
}

.Pinput input{
 width: 430rpx;
 height: 60rpx;
 border: 1rpx solid #E1E1E1;
 border-radius: 30rpx;
 padding-left: 60rpx;
}
.tcount2,.tcount1{
  line-height: 62rpx;
}
.Pinput input.active{
 width: 620rpx;
 transition: all .3s ease-in-out;

}
/*搜索前默认页*/
.se1Title,.se2Title{
  padding: 40rpx 30rpx;
  display: flex;
  justify-content:space-between;
   align-items:center;

}
.se1Title text,.se2Title text{
 font-size: 24rpx;
 color:#000000;
 font-weight: 500;
}
.se1Count,.se2Count{
  padding: 25rpx 30rpx;
}
.se1Count text,.se2Count text{
  min-width: 85rpx;
  height: 58rpx;
  text-align: center;
  line-height: 58rpx;
  font-size: 27rpx;
  color: #686868;
  background: #ffffff;
  border-radius: 58rpx;
  display:inline-block;
  margin:15rpx 30rpx 15rpx 0;
  padding: 0 30rpx;
}
/*固定导航样式*/
.gps{
  background: #FFFFFF;
  display: flex;
  padding: 10rpx 30rpx;
   /* justify-content:space-around;
    align-items:center; */
    transition: all .5s ease-in;
    box-shadow: 0,
     0 ,
      0 0 10rpx rgba(0,0,0,.5),
       0;
}
.gps view{
  font-size: 28rpx;
  color: #2B2B2B;
  flex: 1;
  text-align: center;
  
}
.gps view image{
  position: relative;
  left: 10rpx;
  top:-5rpx;

}
/* 导出职位列表样式 */
.Wxlist{
  background: #FFFFFF;
  height: 100%;
}
@import '/pages/child/CompanyPage/CompanyPage.wxss';
@import '/pages/child/Template/List.wxss';
@import '/pages/child/Template/NESLiss.wxss';
/*其他公司样式*/
.seachCompay{
  background: #FFFFFF;
  padding:0 35rpx;
}
.otherCompany{

  font-size: 28rpx;
  color: #999999;
  display: flex;
  justify-content:space-between;
  align-items:center; 
  padding: 30rpx 0;
  /*border-top:1rpx solid #E1E1E1; */
}
.section1{
  padding: 15rpx 35rpx 0 35rpx;
  margin: 10rpx 0 10rpx 0;
}
.gps .active{
 color: #00b289;
}
/***/
.searchResult{
  background: #FFFFFF;
  padding: 0 30rpx ;
  margin-top: 1rpx;
}
.seacChild{
  height: 95rpx;
  line-height: 95rpx;
  border-bottom: 1rpx solid #F2F2F2;
  color: #686868;
  font-size: 28rpx;
}
.seacChild image{
  position: relative;
  top:3rpx;
  margin-right: 30rpx;
}
.seacChild:nth-last-child(1){
   border-bottom:none;
}
/* 赛选部分样式 */
.preparation {
  padding: 0 30rpx 150rpx 30rpx;
  position: relative;
  background: #F4F4F4;
}
.preTitle{
  display: block;
  padding:15rpx 0; 
  font-size: 27rpx;
  color: #333333;
}
.preCount{
  display:inline-block;
  font-size: 25rpx;
  padding: 19rpx 9rpx;
  background: #FFFFFF;
  border: 1rpx solid #eeeeee;
  border-radius: 5rpx;
  margin: 15rpx 15rpx 15rpx 0;
  color: #666666;
  min-width: 116rpx;
  text-align: center;
}
.preCount.active{
  display:inline-block;
  font-size: 25rpx;
  padding: 19rpx 9rpx;
  background: #02AE81;
  border: 1rpx solid #02AE81;
  border-radius: 5rpx;
  margin: 15rpx 15rpx 15rpx 0;
  color: #FFFFFF;
}
.surse{
  position: fixed;
  width: 90%;
  background: rgba(255,255,255,0);
  bottom: 0;
  display: flex;
  margin: 50rpx 0;
  justify-content:space-between;
  align-items:center; 
}
.usebtn{
  width: 40%;
  color: #FFFFFF;
}
.u1{
  background: #4c98cf;
}
.u2{
 background: #FCA;
}
.section1{
  padding: 20rpx 0;
  border-bottom: 1rpx solid #E1E1E1
}
