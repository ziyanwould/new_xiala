/* pages/child/grabble/grabble.wxss */
/*头部样式*/
page{
  width: 100%;
  height: 100%;
}

.pageAll{
   width: 100%;
  height: 100%;
  background: #F4F4F4;
}
.psection1{
  background: #ffffff;
  padding: 12rpx 30rpx;
  font-size: 28rpx;
  color: #000000;
  display: flex;
  justify-content:space-between;
}
.tcount1{
  position: relative;
  top:-10rpx;
}
.Pinput{
  position: relative;
}
.Pinput image{
  position: absolute;
  left: 15rpx;
  top: 5rpx;
}

.Pinput input{
 width: 430rpx;
 height: 60rpx;
 border: 1rpx solid #E1E1E1;
 border-radius: 30rpx;
 padding-left: 60rpx;
}
.tcount2,.tcount1{
  line-height: 62rpx;
}
/*搜索前默认页*/
.se1Title,.se2Title{
  padding: 40rpx 30rpx;
  display: flex;
  justify-content:space-between;
   align-items:center;

}
.se1Title text,.se2Title text{
 font-size: 24rpx;
 color:#000000;
 font-weight: 500;
}
.se1Count,.se2Count{
  padding: 25rpx 30rpx;
}
.se1Count text,.se2Count text{
  min-width: 85rpx;
  height: 58rpx;
  text-align: center;
  line-height: 58rpx;
  font-size: 27rpx;
  color: #686868;
  background: #ffffff;
  border-radius: 58rpx;
  display:inline-block;
  margin:15rpx 30rpx 15rpx 0;
  padding: 0 30rpx;
}
/*固定导航样式*/
.gps{
  background: #FFFFFF;
  display: flex;
  padding: 10rpx 30rpx;
   /* justify-content:space-around;
    align-items:center; */
    transition: all .5s ease-in;
    box-shadow: 0,
     0 ,
      0 0 10rpx rgba(0,0,0,.5),
       0;
}
.gps view{
  font-size: 28rpx;
  color: #2B2B2B;
  flex: 1;
  text-align: center;
  
}
.gps view image{
  position: relative;
  left: 10rpx;
  top:-5rpx;

}
/* 导出职位列表样式 */
.Wxlist{
  background: #FFFFFF;
}
@import '/pages/child/CompanyPage/CompanyPage.wxss';
@import '/pages/child/Template/List.wxss';
/*其他公司样式*/
.seachCompay{
  background: #FFFFFF;
  padding:0 35rpx;
}
.otherCompany{

  font-size: 28rpx;
  color: #999999;
  display: flex;
  justify-content:space-between;
  align-items:center; 
  padding: 30rpx 0;
  border-top:1rpx solid #E1E1E1; 
}
.section1{
  padding: 15rpx 35rpx 0 35rpx;
  margin: 10rpx 0 10rpx 0;
}
.gps .active{
 color: #00b289;
}