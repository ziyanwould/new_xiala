/* pages/child/ExpectWork/ExpectWork.wxss */
.selsect{
  padding: 0 30rpx;
  border-top: 2rpx solid #F4F4F4;
}
.sel_child{
  display: flex;
  justify-content: space-between;
  align-items:center;
  height: 100rpx;
  border-bottom: 1rpx solid #F4F4F4;
}
.sel_child:last-child{
   border-bottom: none;

}
.sel_right image{
  margin-left: 40rpx;
  width: 11rpx;
  height: 18rpx;
}
.sel_left{
  color: #0e050b;
  font-size: 30rpx;
  white-space:nowrap; 
  overflow:visible; 
  text-overflow:ellipsis;
}
.sel_right text{
  color: #999999;
  font-size: 29rpx;
}
.show{
  padding:30rpx;
  font-size: 26rpx;
  background: #F4F4F4;
  color: #9e9e9e;
}
.text_count{
  padding: 30rpx;
  font-size: 26rpx;
  border-bottom: 2rpx solid #F4F4F4;
}
.save{
  margin-top: 120rpx;
  width: 70%;
  text-align: center;
  background: #46A388;
  color: #FFFFFF;

}


.remove_this{
  margin-top: 50rpx;
  font-size: 24rpx;
  text-align: center;
  color:  #999999;
}

.sel_right input{
text-align: right;
font-size: 28rpx;
color: #a3a3a3;
}