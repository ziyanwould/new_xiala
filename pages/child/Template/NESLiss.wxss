/*新的列表*/
.paTimejob{
  height: 164rpx;
  overflow:hidden;
  padding: 0 30rpx;
  border-bottom: 20rpx solid #f7f8f8;
}
.sec{
  position: relative;
   display: flex;
   justify-content:space-between;
}
.paTimejob .titles{
  font-size: 34rpx;
  font-weight: 500;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}

.paTimejob .own{
   position: relative;
   top: -5rpx;
   font-size: 20rpx;
   display: inline-block;
   color: #ffcd40;
   border: 1rpx solid #ffcd40;
   padding: 0 12rpx;
   border-radius:6rpx; 
   margin-left: 10rpx;
}
.sec_right .pays{
 font-size: 26rpx;
 color: red;
}
.paTimejob .tag{
  font-size: 22rpx;
  color: #b5b5b6;
  background: #EDEDED;
  display: inline-block;
  padding: 0 10rpx;
  margin-right: 15rpx;
  border-radius: 6rpx;

}
.paTimejob .time{
  font-size: 20rpx;
  color: #999999;
}
.paTimejob .sec:nth-of-type(2){
  margin-top: 10rpx;
}
.paTimejob .sec:nth-of-type(1){
  margin-top: 20rpx;
}

/*全职列表*/
.paTimejob.fullTimeJob{
  height: 212rpx;
}
.fullTimeJob{
   position: relative;
   display: flex;
   justify-content: flex-start;
}
.fullLight image{
 width: 120rpx;
 height: 120rpx;
 margin-right: 20rpx;
 margin-top: 46rpx;
 border-radius:120rpx;
}
.fullRight .tagcompany{
  width: 26rpx;
  height: 26rpx;
}
.fullRight .count{
  color: #777777;
  font-size: 28rpx;
  margin-left: 10rpx;
  display: inline-block;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
 

}

.fullRight .distance{
  color: #999999;
  font-size: 22rpx;
}
.paTimejob.fullTimeJob .sec:nth-of-type(2){
  margin-top: 0rpx;
  margin-bottom: 15rpx;
}
.paTimejob.fullTimeJob .sec:nth-of-type(1){
  margin-top: 32rpx;
  padding-bottom: 0rpx;
  margin-bottom: 20rpx;
  height: 30rpx;
}
.paTimejob.fullTimeJob .sec:nth-of-type(3){
  padding-top: 5rpx;
  border-top: 1rpx solid #ECECEC;
  position: relative;
  top: -5rpx;

  /* padding-top: 16rpx; */
}
.paTimejob .fullRight{
  flex: 1;
}

.self{
  display: flex;
  align-items:center;
}